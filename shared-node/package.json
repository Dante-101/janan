{
  "name": "shared-node",
  "version": "1.0.0",
  "description": "Contains code shared code used by node processes",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:dev",
    "build:watch": "tsc --watch",
    "build:dev": "tsc",
    "clean": "rimraf ./build",
    "test": "npm run test:unit && npm run test:intg",
    "test:clean": "npm run clean && npm run build && npm run test",
    "test:unit": "cross-env NODE_PATH=./build NODE_ENV=test mocha --require ./build/shared-node/test/setup \"./build/shared-node/test/unit/**/*.test.js\"",
    "test:intg": "cross-env NODE_PATH=./build NODE_ENV=test mocha --require ./build/shared-node/test/setup \"./build/shared-node/test/integration/**/*.test.js\"",
    "upgrade-dep": "ncu -u --packageFile package.json && npm i && npm run clean && npm run build && npm run test"
  },
  "author": "Gaurav Lahoti",
  "license": "ISC",
  "devDependencies": {
    "@types/archiver": "3.0.0",
    "@types/bluebird": "3.5.27",
    "@types/chai": "4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "@types/extract-zip": "1.6.2",
    "@types/fs-extra": "8.0.0",
    "@types/jsonwebtoken": "8.3.2",
    "@types/lodash": "4.14.136",
    "@types/mime-types": "2.1.0",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.8",
    "@types/request-promise": "4.1.44",
    "@types/sinon": "7.0.13",
    "@types/strip-json-comments": "3.0.0",
    "@types/tar": "4.0.3",
    "@types/url-parse": "1.4.3",
    "@types/uuid": "3.4.5",
    "@types/xml2js": "0.4.4",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "lodash": "4.17.15",
    "mocha": "6.2.0",
