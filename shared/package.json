        "@types/fs-extra": "8.0.0",
        "@types/lodash": "4.14.141",
        "@types/md5": "2.1.33",
        "@types/mocha": "5.2.7",
        "@types/request-promise": "4.1.44",
        "@types/sinon": "7.5.0",
        "@types/uuid": "3.4.5",
        "chai": "4.2.0",
        "fs-extra": "8.1.0",
        "mocha": "6.2.1",
        "typescript": "3.6.3",
        "winston": "3.2.1"
    },
    "dependencies": {
        "aws-sdk": "2.543.0",
        "bluebird": "3.7.0",
        "chance": "1.1.0",
