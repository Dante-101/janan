{
    "name": "shared",
    "version": "1.0.0",
    "description": "Contains code shared by all the tech. Includes code that runs on browsers and nodejs",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "clean": "rimraf ./build*",
        "test": "npm run test:unit",
        "test:clean": "npm run clean && npm run build && npm run test",
        "test:unit": "cross-env NODE_PATH=./build NODE_ENV=test mocha --require ./build/shared/test/setup \"./build/shared/**/*.test.js\"",
        "upgrade-dep": "ncu -u --packageFile package.json && npm i && npm run build && npm run test"
    },
    "author": "Gaurav Lahoti",
    "license": "MIT",
    "devDependencies": {
        "@types/bluebird": "3.5.27",
